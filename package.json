{
  "name": "server",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "tsx watch ./src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node ./dist/server.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.5",
    "nodemon": "^3.1.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.10",
    "@types/nodemon": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "imports": {
    "#controllers/*": "./dist/controllers/*.js",
    "#models/*": "./dist/models/*.js",
    "#routes/*": "./dist/routes/*.js",
    "#utils/*": "./dist/utils/*.js",
    "#config/*": "./dist/config/*.js"
  }
}
